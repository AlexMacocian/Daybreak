<UserControl x:Class="Daybreak.Controls.ImageViewer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Daybreak.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Image x:Name="Image1" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Stretch="Fill"></Image>
        <Image x:Name="Image2" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Stretch="Fill"></Image>
    </Grid>
</UserControl>
